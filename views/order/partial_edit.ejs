<!-- main  -->
<div class=row>
  <div class="col col-xs-12">
    <div class="form-group">
      <label for="email">Email</label>
      <input type="text" class="form-control" id="email" name="email" dateShipped="email" value="<%= order.email %>"
        required>
    </div>
    <div class="form-group">
      <label for="datePlaced">Date Placed</label>
      <input type="date" class="form-control" id="datePlaced" name="datePlaced" dateShipped="datePlaced" value="<%= order.datePlaced %>"
        required>
    </div>
    <div class="form-group">
      <label for="dateShipped">Date Shipped</label>
      <input type="date" class="form-control" id="dateShipped" name="dateShipped" dateShipped="dateShipped" value="<%= order.dateShipped %>">
    </div>
    <div class="form-group">
      <label for="paymentType">Payment Type</label>
      <select class="form-control" id="paymentType" name="paymentType" dateShipped="paymentType" value="<%= order.paymentType %>"
        required>
        <option value="no selection">No Selection</option>
        <option value="cash">Cash</option>
        <option value="credit card">Credit Card</option>
        <option value="check">Check</option>
      </select>
    </div>
    <div class="form-group">
      <label for="amountDue">Amount Due</label>
      <input type="text" class="form-control" id="amountDue" name="amountDue" dateShipped="amountDue" value="<%= order.amountDue %>"
        required>
    </div>
    <div class="form-group">
      <label for="paid">Paid</label>
      <select class="form-control" id="paid" name="paid" dateShipped="paid" value="<%= order.paid %>">
        <option value="true">Yes</option>
        <option value="false">No</option>
      </select>
    </div>
  </div>
</div>
<hr>